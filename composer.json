{
    "name": "blade-ui-kit/blade-heroicons",
    "description": "A package to easily make use of Heroicons in your Laravel Blade views.",
    "keywords": ["Blade", "Heroicons", "Laravel"],
    "homepage": "https://github.com/blade-ui-kit/blade-heroicons",
    "license": "MIT",
    "authors": [
        {
            "name": "Dries Vints",
            "homepage": "https://driesvints.com"
        }
    ],
    "require": {
        "php": "^7.4 || ^8.0",
        "blade-ui-kit/blade-icons": "dev-abstract-generator-command",
        "illuminate/support": "^8.0"
    },
    "require-dev": {
        "orchestra/testbench": "^6.0",
        "phpunit/phpunit": "^9.0"
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "BladeUI\\Heroicons\\BladeHeroiconsServiceProvider"
            ]
        }
    },
    "autoload": {
        "psr-4": {
            "BladeUI\\Heroicons\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests"
        }
    },
    "repositories": [
        {
            "type": "vcs",
            "url": "https://github.com/mallardduck/blade-icons"
        }
    ],
    "minimum-stability": "dev",
    "prefer-stable": true,
    "scripts": {
        "generate": "./bin/generate"
    }
}
